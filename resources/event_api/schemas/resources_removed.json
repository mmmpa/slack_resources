{
  "$schema": "http://json-schema.org/draft-07/schema",
  "definitions": {
    "token": {
      "type": "string"
    },
    "team_id": {
      "type": "string"
    },
    "api_app_id": {
      "type": "string"
    },
    "resource_type": {
      "type": "string"
    },
    "grant_type": {
      "type": "string"
    },
    "resource_id": {
      "type": "string"
    },
    "grant": {
      "type": "object",
      "description": "(defined by script)",
      "properties": {
        "type": {
          "$ref": "#/definitions/grant_type"
        },
        "resource_id": {
          "$ref": "#/definitions/resource_id"
        }
      }
    },
    "resource": {
      "type": "object",
      "description": "(defined by script)",
      "properties": {
        "type": {
          "$ref": "#/definitions/resource_type"
        },
        "grant": {
          "$ref": "#/definitions/grant"
        }
      }
    },
    "scope": {
      "type": "string",
      "enum": [
        "channels:read",
        "channels:history",
        "groups:history",
        "mpim:history",
        "dnd:read",
        "team:read",
        "emoji:read",
        "files:read",
        "groups:read",
        "im:read",
        "im:history",
        "links:read",
        "pins:read",
        "reactions:read",
        "stars:read",
        "usergroups:read",
        "users:read"
      ]
    },
    "resource_item": {
      "type": "object",
      "description": "(defined by script)",
      "properties": {
        "resource": {
          "$ref": "#/definitions/resource"
        },
        "scopes": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/scope"
          }
        }
      }
    },
    "resources_removed_event": {
      "type": "object",
      "description": "(defined by script)",
      "properties": {
        "type": {
          "const": "resources_removed"
        },
        "resources": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/resource_item"
          }
        }
      }
    },
    "event_id": {
      "type": "string"
    },
    "time_integer": {
      "type": "number"
    },
    "resources_removed": {
      "type": "object",
      "description": "learn more: https://api.slack.com/events/resources_removed",
      "properties": {
        "token": {
          "$ref": "#/definitions/token"
        },
        "team_id": {
          "$ref": "#/definitions/team_id"
        },
        "api_app_id": {
          "$ref": "#/definitions/api_app_id"
        },
        "event": {
          "$ref": "#/definitions/resources_removed_event"
        },
        "type": {
          "const": "event_callback"
        },
        "authed_teams": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/team_id"
          }
        },
        "event_id": {
          "$ref": "#/definitions/event_id"
        },
        "event_time": {
          "$ref": "#/definitions/time_integer"
        }
      }
    }
  }
}