{
  "$schema": "http://json-schema.org/draft-07/schema",
  "definitions": {
    "tokens": {
      "oauth": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "tokens_revoked": {
      "type": "object",
      "description": "learn more: https://api.slack.com/events/tokens_revoked",
      "properties": {
        "type": {
          "const": "tokens_revoked"
        },
        "tokens": {
          "$ref": "#/definitions/tokens"
        }
      },
      "example": {
        "type": "tokens_revoked",
        "tokens": {
          "oauth": [
            "UXXXXXXXX"
          ],
          "bot": [
            "UXXXXXXXX"
          ]
        }
      }
    }
  }
}