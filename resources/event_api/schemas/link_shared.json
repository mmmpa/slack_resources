{
  "$schema": "http://json-schema.org/draft-07/schema",
  "definitions": {
    "token": {
      "type": "string"
    },
    "team_id": {
      "type": "string"
    },
    "api_app_id": {
      "type": "string"
    },
    "channel_id": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string"
    },
    "link": {
      "type": "object",
      "description": "(defined by script)",
      "properties": {
        "domain": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "link_shared_event": {
      "type": "object",
      "description": "(defined by script)",
      "properties": {
        "type": {
          "const": "link_shared"
        },
        "channel": {
          "$ref": "#/definitions/channel_id"
        },
        "user": {
          "$ref": "#/definitions/user_id"
        },
        "message_ts": {
          "$ref": "#/definitions/timestamp"
        },
        "thread_ts": {
          "$ref": "#/definitions/timestamp"
        },
        "links": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/link"
          }
        }
      }
    },
    "event_id": {
      "type": "string"
    },
    "time_integer": {
      "type": "number"
    },
    "link_shared": {
      "type": "object",
      "description": "learn more: https://api.slack.com/events/link_shared",
      "properties": {
        "token": {
          "$ref": "#/definitions/token"
        },
        "team_id": {
          "$ref": "#/definitions/team_id"
        },
        "api_app_id": {
          "$ref": "#/definitions/api_app_id"
        },
        "event": {
          "$ref": "#/definitions/link_shared_event"
        },
        "type": {
          "const": "event_callback"
        },
        "authed_users": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/user_id"
          }
        },
        "event_id": {
          "$ref": "#/definitions/event_id"
        },
        "event_time": {
          "$ref": "#/definitions/time_integer"
        }
      }
    }
  }
}